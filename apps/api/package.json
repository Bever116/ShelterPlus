{
  "name": "@shelterplus/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm run prisma:generate && nest build",
    "prisma:generate": "cross-env PRISMA_GENERATE_SKIP_AUTOINSTALL=1 prisma generate",
    "start": "pnpm run prisma:generate && nest start",
    "start:dev": "pnpm run prisma:generate && nest start --watch",
    "dev": "pnpm run start:dev",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "diagnose:official-config": "node scripts/diagnose-official-config.cjs"
  },
  "dependencies": {
    "@nestjs/common": "10.3.2",
    "@nestjs/core": "10.3.2",
    "@nestjs/platform-express": "10.3.2",
    "@nestjs/config": "3.1.1",
    "@nestjs/axios": "3.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "@prisma/client": "5.9.1",
    "@nestjs/passport": "10.0.2",
    "passport": "0.7.0",
    "passport-discord": "0.1.4",
    "passport-http-bearer": "1.0.1",
    "@nestjs/jwt": "10.2.0",
    "cookie-parser": "1.4.6",
    "express-session": "1.17.3",
    "bcrypt": "5.1.1",
    "uuid": "9.0.1",
    "seedrandom": "3.0.5",
    "discord.js": "14.14.1",
    "dotenv": "16.4.5",
    "@nestjs/websockets": "10.3.2",
    "@nestjs/platform-socket.io": "10.3.2",
    "socket.io": "4.7.5",
    "@shelterplus/shared": "workspace:*"
  },
  "devDependencies": {
    "@nestjs/cli": "10.2.1",
    "@nestjs/schematics": "10.1.0",
    "@nestjs/testing": "10.3.2",
    "cross-env": "7.0.3",
    "@types/seedrandom": "3.0.4",
    "@types/cookie-parser": "1.4.6",
    "@types/express-session": "1.17.9",
    "@types/uuid": "9.0.7",
    "prisma": "5.9.1",
    "typescript": "5.3.3",
    "ts-node": "10.9.2",
    "ts-loader": "9.5.1",
    "tsconfig-paths": "4.2.0",
    "eslint": "8.57.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0"
  }
}
